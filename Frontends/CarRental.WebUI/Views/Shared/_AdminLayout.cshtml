@{
    Layout = null;
}

<!DOCTYPE html>
<html dir="ltr" lang="en">

@await Component.InvokeAsync("HeadPageAdminLayoutComponentPartial")


<body>
    <div class="preloader">
        <div class="lds-ripple">
            <div class="lds-pos"></div>
            <div class="lds-pos"></div>
        </div>
    </div>
    <div id="main-wrapper">
        <!--Aside-->
        @await Component.InvokeAsync("AsideAdminLayoutComponentPartial")
        @await Component.InvokeAsync("HeaderAdminLayoutComponentPartial")

        <div class="page-wrapper">

            <div class="page-breadcrumb">
                <div class="row">
                    <div class="col-12 d-flex no-block align-items-center">
                        <h4 class="page-title">@ViewData["Title"]</h4>
                        <div class="ml-auto text-right">
                            <nav aria-label="breadcrumb">
                                <ol class="breadcrumb">
                                    <li class="breadcrumb-item"><a href="#">Home</a></li>
                                    <li class="breadcrumb-item active" aria-current="page">Library</li>
                                </ol>
                            </nav>
                        </div>
                    </div>
                </div>
            </div>

            <div class="container-fluid">

                @RenderBody()

            </div>

            @await Component.InvokeAsync("FooterAdminLayoutComponentPartial")


        </div>

    </div>

    @await Component.InvokeAsync("ScriptAdminLayoutComponentPartial")


</body>

</html>
